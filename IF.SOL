pragma solidity ^0.8.20;

contract ConditionTest {
    function checkNumber(uint256 x) public pure returns (string memory) {
        if (x < 10) {
            return unicode"小于 10";
        } else if (x < 100) {
            return "10 ~ 99";      // 这里是纯英文，可以不加 unicode
        } else {
            return unicode"大于 99";
        }
    }
}
